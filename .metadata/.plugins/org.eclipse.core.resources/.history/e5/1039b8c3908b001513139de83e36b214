//--------------------------------------------------------------------------------------------------
//=== Includes =====================================================================================
//--------------------------------------------------------------------------------------------------
#include "../LIBS/SysTick.h"

//--------------------------------------------------------------------------------------------------
//=== Global Functions =============================================================================
//--------------------------------------------------------------------------------------------------
void DelayMs(uint32_t millis)
{
	uint32_t i, TIME_MULTIPLIER=2500;

	for (i=0; i < millis*TIME_MULTIPLIER; i++)
	{
		;
	}
}
